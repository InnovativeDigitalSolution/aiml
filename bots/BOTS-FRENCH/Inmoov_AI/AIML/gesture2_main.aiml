<?xml version="1.0" encoding="UTF-8"?>
<aiml>

<category><pattern>ouvre la main *</pattern>
<template><think><set name="main"><star index="1" /></set></think>
<condition>
<li name="main" value="droite"><think><set name="main">right</set></think></li>
<li><think><set name="main">left</set></think></li>
</condition>
<oob>
				<mrl>
				   <service>python</service>
				   <method>exec</method>
				   <param>MoveHand('<get name="main"/>',0,0,0,0,0)</param>
				</mrl>
</oob>
<random>
		<li>Main <star index="1" /> ouverte</li>
		<li>C'est fait</li>
</random>
</template>
</category> 


<category><pattern>ferme la main *</pattern>
<template><think><set name="main"><star index="1" /></set></think>
<condition>
<li name="main" value="droite"><think><set name="main">right</set></think></li>
<li><think><set name="main">left</set></think></li>
</condition>
<oob>
				<mrl>
				   <service>python</service>
				   <method>exec</method>
				   <param>MoveHand('<get name="main"/>',180,180,180,180,180)</param>
				</mrl>
</oob>
<random>
		<li>Main <star index="1" /> fermée</li>
		<li>C'est fait</li>
		<li>OK</li>
		<li>Voilà chef !!!</li>
</random>
</template>
</category> 

<category><pattern>lève l index *</pattern>
<template><think><set name="main"><star index="1" /></set></think>
<condition>
<li name="main" value="droit"><think><set name="main">right</set></think></li>
<li><think><set name="main">left</set></think></li>
</condition>
<oob>
				<mrl>
				   <service>python</service>
				   <method>exec</method>
				   <param>MoveHand('<get name="main"/>',-1,0,-1,-1,-1)</param>
				</mrl>
</oob>
<random>
		<li>index <star index="1" /> ouvert</li>
		<li>C'est fait</li>
		<li>Tu me fait faire ma gym du matin</li>
</random>
</template>
</category> 

<category><pattern>baisse l index *</pattern>
<template><think><set name="main"><star index="1" /></set></think>
<condition>
<li name="main" value="droit"><think><set name="main">right</set></think></li>
<li><think><set name="main">left</set></think></li>
</condition>
<oob>
				<mrl>
				   <service>python</service>
				   <method>exec</method>
				   <param>MoveHand('<get name="main"/>',-1,0,-1,-1,-1)</param>
				</mrl>
</oob>
<random>
		<li>index <star index="1" /> baissé</li>
		<li>C'est fait</li>
		<li>j'ai pas de souci articulation</li>
</random>
</template>
</category>
 


<category><pattern>FAIS UN DOIGT</pattern>
