<category><pattern>ouvre l index *</pattern>
<template><srai>lève l index <star index="1" /></srai></template>
</category>

<category><pattern>ferme l index *</pattern>
<template><srai>baisse l index <star index="1" /></srai></template>
</category>

<category><pattern>C EST UN DOIGT</pattern>
