<?xml version="1.0" encoding="UTF-8"?>
<aiml>

<category><pattern>PLAY *</pattern>
<template>
<li>It's a good idea, pease chose one video</li>
<think>
            <set name="PlayUtub"><star/></set>
         </think>
	<oob>
					<mrl>
				   <service>python</service>
				   <method>exec</method>
				   <param>PlayUtub("<star/>",0)</param>
				 </mrl>
			</oob>
</template>
</category> 

  
<category><pattern># NUMBER *</pattern>
<template>
	<oob>
					<mrl>
				   <service>python</service>
				   <method>exec</method>
				   <param>PlayUtub("<get name="PlayUtub"/>",<star index="2" />)</param>
				 </mrl>
			</oob>
Great choice
</template>
</category>

<category><pattern># VIDEO *</pattern>
<template><srai># NUMBER <star index="2" /></srai></template>
</category>


<category><pattern>STOP</pattern><that># Great choice #</that>
<template>
	<oob>
					<mrl>
				   <service>python</service>
				   <method>exec</method>
				   <param>PlayUtub("stop",0)</param>
				 </mrl>
			</oob>
</template>
</category>    




</aiml>

